{
    "name": "alisaleem27/nova-dropzone-field",
    "description": "A Laravel Nova field.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0"
    },
    "autoload": {
        "psr-4": {
            "AliSaleem\\NovaDropzoneField\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "AliSaleem\\NovaDropzoneField\\FieldServiceProvider"
            ]
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "laravel/nova": "^4.21"
    }
}
